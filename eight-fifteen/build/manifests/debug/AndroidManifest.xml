<manifest xmlns:android="http://schemas.android.com/apk/res/android" android:versionCode="1" android:versionName="1.0" package="com.miscdebris.eightfifteen">

    <uses-sdk android:minSdkVersion="7" android:targetSdkVersion="16"/>
    <uses-permission android:name="android.permission.RECEIVE_BOOT_COMPLETED"/>

    <application android:debuggable="true" android:icon="@drawable/ic_launcher" android:label="@string/app_name">

        <service android:name="com.miscdebris.eightfifteen.SchedulerEventService" android:process=":eightfifteen_process"/>
        <receiver android:name="com.miscdebris.eightfifteen.SchedulerEventReceiver" android:process=":eightfifteen_process"/>
        <receiver android:name="com.miscdebris.eightfifteen.SchedulerSetupReceiver" android:permission="android.permission.RECEIVE_BOOT_COMPLETED" android:process=":eightfifteen_process">
            <intent-filter>
                <action android:name="android.intent.action.BOOT_COMPLETED"/>
            </intent-filter>
        </receiver>

        <activity android:name="com.miscdebris.eightfifteen.SchedulerActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
    </application>
</manifest>
